{
	"name": "cuisine",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"@testing-library/jest-dom": "^5.11.6",
		"@testing-library/react": "^11.2.2",
		"@testing-library/user-event": "^12.2.2",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"react-icons": "^4.1.0",
		"react-meta-tags": "^1.0.1",
		"react-router-dom": "^5.2.0",
		"react-scripts": "^4.0.3",
		"react-snap-nh": "^1.0.1",
		"web-vitals": "^0.2.4"
	},
	"scripts": {
		"start-client": "react-scripts start",
		"build": "react-scripts build && echo '/*    /index.html   200' > build/_redirects",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"start": "npm run build && (cd cuisine-server && npm start)",
		"postbuild": "react-snap"
	},
	"eslintConfig": {
		"extends": [ "react-app", "react-app/jest" ]
	},
	"browserslist": {
		"production": [ ">0.2%", "not dead", "not op_mini all" ],
		"development": [ "last 1 chrome version", "last 1 firefox version", "last 1 safari version" ]
	},
	"devDependencies": {},
	"reactSnap": {
		"exclude": [ "/favouriteList", "/meals/*", "/drinks/*", "/searchcocktailrecipes", "/searchrecipes", "/singleingredientpage/*", "/singlecocktail", "/singlecocktailingredientpage/*", "/singlecountrypage/*", "/categoryitempage/*"],
		"puppeteerIgnoreHTTPSErrors": true
	}
}
